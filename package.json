{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "6.0.0"
  },
  "scripts": {
    "start": "hexo serve",
    "build": "npm run build-client && NODE_ENV=production hexo generate && cp source/_redirects public/",
    "start-client": "parcel bundle/init.js",
    "build-client": "parcel build bundle/init.js --dist-dir public/bundle --public-url /bundle/ --no-source-maps"
  },
  "dependencies": {
    "hexo": "^6.0.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^4.0.0",
    "hexo-renderer-stylus": "^2.0.0",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^0.0.3",
    "lazysizes": "^5.2.1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.2.1",
    "parcel": "^2.2.1"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
